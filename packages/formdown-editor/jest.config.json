{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
        "<rootDir>/src",
        "<rootDir>/__tests__"
    ],
    "testMatch": [
        "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
        "src/**/*.ts",
        "!src/**/*.d.ts",
        "!src/vite-env.d.ts"
    ]
}