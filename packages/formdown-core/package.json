{
  "name": "@formdown/core",
  "version": "0.4.0",
  "description": "Core parsing and generation engine for Formdown",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.js"
    },
    "./generator": {
      "types": "./dist/generator.d.ts",
      "import": "./dist/generator.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./form-manager": {
      "types": "./dist/form-manager.d.ts",
      "import": "./dist/form-manager.js"
    },
    "./extensions": {
      "types": "./dist/extensions/index.d.ts",
      "import": "./dist/extensions/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ../../node_modules/typescript/bin/tsc",
    "test": "node ../../node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "core",
    "parser",
    "generator"
  ],
  "author": {
    "name": "iyulab",
    "url": "https://www.iyulab.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyulab/formdown.git",
    "directory": "packages/formdown-core"
  },
  "homepage": "https://formdown.dev",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "marked": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}